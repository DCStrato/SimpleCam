            <ComboBox Name="SerialIP1" Uid="1" Grid.Column="2"  FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Grid.Row="2" Width="76" Margin="4,0,0,0"  >
                <ComboBoxItem Tag="1" Content = "Network" FontWeight="Bold"/>
                <ComboBoxItem Tag="2" Content = "Serial " IsSelected="True"   FontWeight="Bold" />
            </ComboBox>
            <ComboBox Name="SerialIP2" Uid="2" Grid.Column="2"  FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Grid.Row="3" Width="76" Margin="4,0,0,0"  >
                <ComboBoxItem Content = "Network" IsSelected="True" FontWeight="Bold" />
                <ComboBoxItem Content = "Serial "  FontWeight="Bold" />
            </ComboBox>
            <ComboBox Name="SerialIP3" Uid="3" Grid.Column="2" FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Grid.Row="4" Width="76" Margin="4,0,0,0"   >
                <ComboBoxItem Content = "Network" IsSelected="True"  FontWeight="Bold"/>
                <ComboBoxItem Content = "Serial "  FontWeight="Bold" />
            </ComboBox>
            <ComboBox Name="SerialIP4" Uid="4" Grid.Column="2"  FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Grid.Row="5" Width="76" Margin="4,0,0,0" >
                <ComboBoxItem Content = "Network" IsSelected="True"  FontWeight="Bold"/>
                <ComboBoxItem Content = "Serial "  FontWeight="Bold" />
            </ComboBox>
            <ComboBox Name="SerialIP5" Uid="5" Grid.Column="2"  FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Grid.Row="6" Width="76" Margin="4,0,0,0"  >
                <ComboBoxItem Content = "Network" IsSelected="True" FontWeight="Bold" />
                <ComboBoxItem Content = "Serial "  FontWeight="Bold" />
            </ComboBox>
        </Grid>
