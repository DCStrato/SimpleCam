            <CheckBox Name ="Camera2Enable" HorizontalAlignment="Right" Grid.Row="3" VerticalContentAlignment="Bottom" Width="16" FontSize="12" Uid="2" Margin="0,5" Grid.Column="1"/>
            <CheckBox Name ="Camera3Enable" HorizontalAlignment="Right" Grid.Row="4" VerticalContentAlignment="Bottom" Width="16" FontSize="12" Uid="3" Margin="0,5" Grid.Column="1"/>
            <CheckBox Name ="Camera4Enable" HorizontalAlignment="Right" Grid.Row="5" VerticalContentAlignment="Bottom" Width="16" FontSize="12" Uid="4" Margin="0,5" Grid.Column="1"/>
            <CheckBox Name ="Camera5Enable" HorizontalAlignment="Right" Grid.Row="6" VerticalContentAlignment="Bottom" Width="16" FontSize="12" Uid="5" Margin="0,5" Grid.Column="1"/>
            <TextBox Name ="CameraIP1" Style="{StaticResource SetStyle}" Visibility="Hidden" Grid.Column="4" MaxLength="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Grid.Row="2" Text="000.000.000.000" Uid="1" Height="28"/>
            <TextBox Name ="CameraIP2" Style="{StaticResource SetStyle}" Visibility="Visible" Grid.Column="4" MaxLength="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Grid.Row="3" Text="000.000.000.000" Uid="2" Height="28"/>
            <TextBox Name ="CameraIP3" Style="{StaticResource SetStyle}" Visibility="Visible" Grid.Column="4" MaxLength="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Grid.Row="4" Text="000.000.000.000" Uid="3" Height="28"/>
            <TextBox Name ="CameraIP4" Style="{StaticResource SetStyle}" Visibility="Visible" Grid.Column="4" MaxLength="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Grid.Row="5" Text="000.000.000.000" Uid="4" Height="28"/>
            <TextBox Name ="CameraIP5" Style="{StaticResource SetStyle}" Visibility="Visible" Grid.Column="4" MaxLength="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Grid.Row="6" Text="000.000.000.000" Uid="5" Height="28"/>
            <TextBox Name ="CameraPort1" Style="{StaticResource SetStyle}" Visibility="Hidden" MaxLength="4" HorizontalContentAlignment="Center" Grid.Column="5" Grid.Row="2" Text="1259" VerticalContentAlignment="Center" VerticalAlignment="Center" Uid="1" Height="28"/>
            <TextBox Name ="CameraPort2" Style="{StaticResource SetStyle}" Visibility="Visible" MaxLength="4" HorizontalContentAlignment="Center" Grid.Column="5" Grid.Row="3" Text="1259" VerticalContentAlignment="Center" VerticalAlignment="Center" Uid="2" Height="28"/>
            <TextBox Name ="CameraPort3" Style="{StaticResource SetStyle}" Visibility="Visible" MaxLength="4" HorizontalContentAlignment="Center" Grid.Column="5" Grid.Row="4" Text="1259" VerticalContentAlignment="Center" VerticalAlignment="Center" Uid="3" Height="28"/>
            <TextBox Name ="CameraPort4" Style="{StaticResource SetStyle}" Visibility="Visible" MaxLength="4" HorizontalContentAlignment="Center" Grid.Column="5" Grid.Row="5" Text="1259" VerticalContentAlignment="Center" VerticalAlignment="Center" Uid="4" Height="28"/>
            <TextBox Name ="CameraPort5" Style="{StaticResource SetStyle}" Visibility="Visible" MaxLength="4" HorizontalContentAlignment="Center" Grid.Column="5" Grid.Row="6" Text="1259" VerticalContentAlignment="Center" VerticalAlignment="Center" Uid="5" Height="28"/>
            <ComboBox Name="CameraCom1" Visibility="Visible" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Width="76" Grid.Row="2" Grid.Column="5" FontWeight="Bold" VerticalContentAlignment="Center" 
