                <Label Content="{Binding Value, ElementName=CamAdd2, UpdateSourceTrigger=PropertyChanged}" Background="White" FontSize="12" DockPanel.Dock="Right" VerticalContentAlignment="Top" Margin="84,0,-1,0" FontWeight="Bold" />
            </Grid>
            <Grid Name="CameraAdd3" Visibility="Hidden" Grid.Row="4" Grid.Column="4" Margin="2,1" >
                <Slider Name="CamAdd3" Minimum="1" Maximum="7"  TickFrequency="1" TickPlacement="TopLeft"  Foreground="White" IsSnapToTickEnabled="True" HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="83"/>
                <Label Content="{Binding Value, ElementName=CamAdd3, UpdateSourceTrigger=PropertyChanged}" Background="White" FontSize="12" DockPanel.Dock="Right" VerticalContentAlignment="Top" Margin="84,0,-1,0" FontWeight="Bold" />
            </Grid>
            <Grid Name="CameraAdd4" Visibility="Hidden" Grid.Row="5" Grid.Column="4" Margin="2,1" >
                <Slider Name="CamAdd4" Minimum="1" Maximum="7"  TickFrequency="1" TickPlacement="TopLeft"  Background="White" IsSnapToTickEnabled="True" HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="83"/>
                <Label Content="{Binding Value, ElementName=CamAdd4, UpdateSourceTrigger=PropertyChanged}" Background="White" FontSize="12" DockPanel.Dock="Right" VerticalContentAlignment="Top" Margin="84,0,-1,0" FontWeight="Bold" />
            </Grid>
            <Grid Name="CameraAdd5" Visibility="Hidden" Grid.Row="6" Grid.Column="4" Margin="2,1" >
                <Slider Name="CamAdd5" Minimum="1" Maximum="7"  TickFrequency="1" TickPlacement="TopLeft"  Foreground="White" IsSnapToTickEnabled="True" HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="83"/>
